datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
}

model User {
  id       Int     @id @default(autoincrement())
  email    String  @unique
  password String?
  missions Mission[]
}

model Mission {
  id         Int     @id @default(autoincrement())
  title       String
  data       String
  modifiedAt DateTime @default(now())
  userId    Int
  user      User    @relation(fields: [userId], references: [id])
}
